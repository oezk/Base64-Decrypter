<Window x:Class="Base64_Decrypter.DonateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Base64_Decrypter"
        mc:Ignorable="d"
		FontSize="13"
		Background="Gainsboro"
		WindowStartupLocation="CenterOwner"
		Icon="/broken-link.ico"
        Title="Donate" Height="120" Width="270">
	<Grid>
		<TextBlock HorizontalAlignment="Left" Margin="60,10,0,0" TextWrapping="Wrap" Text="Thank you for donating!" VerticalAlignment="Top" Width="142"/>
		<Button Name ="PaypalBtn" Click="btnClick" Content="Paypal" HorizontalAlignment="Left" Margin="41,43,0,0" VerticalAlignment="Top" Width="75"/>
		<Button Name ="VenmoBtn" Click="btnClick" Content="Venmo" HorizontalAlignment="Left" Margin="140,43,0,0" VerticalAlignment="Top" Width="75"/>
	</Grid>
</Window>
